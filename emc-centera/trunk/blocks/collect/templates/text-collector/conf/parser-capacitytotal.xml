<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://www.watch4net.com/OutputParsingLibrary" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.watch4net.com/OutputParsingLibrary collectingconfiguration.xsd ">
	<max-threads>2</max-threads>
	<parser-config>
		<parser-class>com.watch4net.apg.v2.collector.plugins.outputparsing.parsing.generictext.RegexTextParser</parser-class>
		<tree-type>regex-group-tree</tree-type>
		<tree-schema>
			<name>CLUSTER_</name>
			<leaf>
				<name>TotalRawCapacity</name>
				<group>clustercapacity-1</group>
			</leaf>
			<leaf>
				<name>TotalRawCapacityUnit</name>
				<group>clustercapacity-2</group>
			</leaf>
			<leaf>
				<name>UsedRawCapacity</name>
				<group>clustercapacity-3</group>
			</leaf>
			<leaf>
				<name>UsedRawCapacityUnit</name>
				<group>clustercapacity-4</group>
			</leaf>
			<leaf>
				<name>FreeRawCapacity</name>
				<group>clustercapacity-5</group>
			</leaf>
			<leaf>
				<name>FreeRawCapacityUnit</name>
				<group>clustercapacity-6</group>
			</leaf>
			<leaf>
				<name>UsedRawCapacity</name>
				<group>clustercapacity-7</group>
			</leaf>
			<leaf>
				<name>UsedRawCapacityUnit</name>
				<group>clustercapacity-8</group>
			</leaf>
			<leaf>
				<name>SystemResources</name>
				<group>clustercapacity-9</group>
			</leaf>
			<leaf>
				<name>SystemResourcesUnit</name>
				<group>clustercapacity-10</group>
			</leaf>
			<leaf>
				<name>OfflineCapacity</name>
				<group>clustercapacity-11</group>
			</leaf>
			<leaf>
				<name>OfflineCapacityUnit</name>
				<group>clustercapacity-12</group>
			</leaf>
			<leaf>
				<name>SpareCapacity</name>
				<group>clustercapacity-13</group>
			</leaf>
			<leaf>
				<name>SpareCapacityUnit</name>
				<group>clustercapacity-14</group>
			</leaf>
			<leaf>
				<name>UsedCapacity</name>
				<group>clustercapacity-15</group>
			</leaf>
			<leaf>
				<name>UsedCapacityUnit</name>
				<group>clustercapacity-16</group>
			</leaf>
			<leaf>
				<name>AuditAndMetadataCapacity</name>
				<group>clustercapacity-17</group>
			</leaf>
			<leaf>
				<name>AuditAndMetadataUnit</name>
				<group>clustercapacity-18</group>
			</leaf>
			<leaf>
				<name>ProtectedUserData</name>
				<group>clustercapacity-19</group>
			</leaf>
			<leaf>
				<name>ProtectedUserDataUnit</name>
				<group>clustercapacity-20</group>
			</leaf>
			<leaf>
				<name>UserObjectCount</name>
				<group>clustercapacity-21</group>
			</leaf>
			<leaf>
				<name>UserObjectCountUnit</name>
				<group>clustercapacity-22</group>
			</leaf>
		</tree-schema>
		<regex-group-config group="-1">
			<regex name="main">(?s)Total Raw Capacity:(.*?)(?=(Total Raw Capacity:|$))</regex>
			<sub-regex group="1">
				<regex name="clustercapacity">(?s)\s*([\d\.\,]+)\s*(.B|K|M).*Used Raw Capacity:\s+([\d\.\,]+)\s*(.B|K|M).*Free Raw Capacity:\s+([\d\.\,]+)\s*(.B|K|M).*Used Raw Capacity:\s+([\d\.\,]+)\s*(.B|K|M).*System Resources:\s+([\d\.\,]+)\s*(.B|K|M).*Offline Capacity:\s+([\d\.\,]+)\s*(.B|K|M).*Spare Capacity:\s+([\d\.\,]+)\s*(.B|K|M).*Used Capacity:\s+([\d\.\,]+)\s*(.B|K|M).*Audit &amp; Metadata:\s+([\d\.\,]+)\s*(.B|K|M).*Protected User Data:\s+([\d\.\,]+)\s*(.B|K|M).*Used Object Count:\s+([\d\.\,]+)\s*(.B|K|M).*</regex>
			</sub-regex>
		</regex-group-config>
	</parser-config>
	<data-formatter-config>com.watch4net.apg.v2.collector.plugins.textoutputcollector.formatting.RawValueFormatter</data-formatter-config>
	<data-handler-config>com.watch4net.apg.v2.collector.plugins.textoutputcollector.handling.DeltaRateRawValueHandler</data-handler-config>
	<include-contexts>conf/context-centera.xml</include-contexts>
	<data-config>
		<data-accessor>com.watch4net.apg.v2.collector.plugins.outputparsing.executing.local.CLICommandExecutor</data-accessor>
		<stop-on-first-result>true</stop-on-first-result>
		<host-group-config>
			<custom-group>
				<group-name>Group</group-name>
				<host-config>
					<host-list>@host</host-list>
				</host-config>
			</custom-group>
		</host-group-config>
		<command-configuration>
			<command-list>
				<command>"@clipath\\..\\_jvm\\bin\\java" -cp "@clipath\\CenteraViewer.jar" com.filepool.remote.cli.CLI -u @username -p @password -ip (host) -script conf/showcapacitytotal_script.cli</command>
			</command-list>
		</command-configuration>
	</data-config>
</configuration>