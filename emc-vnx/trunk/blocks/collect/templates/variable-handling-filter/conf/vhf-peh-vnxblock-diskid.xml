<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.watch4net.com/VariableHandlingFilterPropertyExtractionHandler" xsi:schemaLocation="http://www.watch4net.com/VariableHandlingFilterPropertyExtractionHandler ./property-extraction-handler.xsd">
  <properties>
    <!-- ex: part=Bus 1 Enclosure 1  Disk 0 
	please note the extra space -->
    <list-property name="part" separator=" " keep="true">
      <part name="busname" size="1"/>
      <part name="space1" size="1"/>
      <part name="busid" size="1"/>
      <part name="space2" size="1"/>
      <part name="encname" size="1"/>
      <part name="space3" size="1"/>
      <part name="encid" size="1"/>
      <part name="space4" size="1"/>
      <part name="space5" size="1"/>
      <part name="diskname" size="1"/>
      <part name="space6" size="1"/>
      <part name="diskid" size="1"/>
    </list-property>
  </properties>
</config>
