			</leaf>
			<leaf>
				<name>rovdms</name>
				<group>savvol-13</group>
			</leaf>
			<leaf>
				<name>checkpointof</name>
				<group>savvol-14</group>
			</leaf>
			<leaf>
				<name>date</name>
				<group>savvol-15</group>
			</leaf>
			<leaf>
				<name>deduplication</name>
				<group>savvol-16</group>
			</leaf>
			<leaf>
				<name>savvolsize</name>
				<group>savvol-17</group>
			</leaf>
			<leaf>
				<name>savvolavailable</name>
				<group>savvol-18</group>
			</leaf>
			<leaf>
				<name>savvolused</name>
