<config xmlns="http://www.watch4net.com/Collecting/InlineCalculationFilter1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.watch4net.com/Collecting/InlineCalculationFilter1 inline-calculation-filter.xsd">
	<result name="OverheadCapacity" unit="GB" time-window="300s">
		<input>MetaVolumeCapacity</input>
		<input>Capacity</input>
		<group-by>device</group-by>
		<group-by>serialnb</group-by>
		<group-by>sstype</group-by>
		<group-by>partid</group-by>
		<filter>device &amp; serialnb &amp; !type=='rawfs' &amp; internal=='false' &amp; parttype=='FileSystem' &amp; datagrp=='VNXFile-FileSystemUsage-CapacityInfo-CheckpointInfo'</filter>
		<jexl-formula>
			<![CDATA[MetaVolumeCapacity - Capacity]]>
		</jexl-formula>
	</result>
	<result name="FreeCapacity" unit="GB" time-window="300s">
		<input>Capacity</input>
		<input>UsedCapacity</input>
		<group-by>device</group-by>
		<group-by>serialnb</group-by>
		<group-by>sstype</group-by>
		<group-by>partid</group-by>
		<group-by>parttype</group-by>
		<filter>device &amp; serialnb &amp; !type=='rawfs' &amp; internal=='false' &amp; parttype=='FileSystem' &amp; datagrp=='VNXFile-FileSystemUsage-CapacityInfo-CheckpointInfo'</filter>
		<jexl-formula>
			<![CDATA[Capacity - UsedCapacity]]>
		</jexl-formula>
	</result>
	<result name="CurrentUtilization" unit="%" time-window="300s">
		<input>Capacity</input>
		<input>UsedCapacity</input>
		<group-by>device</group-by>
		<group-by>serialnb</group-by>
		<group-by>sstype</group-by>
		<group-by>partid</group-by>
		<group-by>parttype</group-by>
		<filter>device &amp; serialnb &amp; !type=='rawfs' &amp; internal=='false' &amp; parttype=='FileSystem' &amp; datagrp=='VNXFile-FileSystemUsage-CapacityInfo-CheckpointInfo'</filter>
		<jexl-formula>
			<![CDATA[UsedCapacity / Capacity * 100 ]]>
		</jexl-formula>
	</result>
	<result name="FreeCapacity" unit="GB" time-window="300s">
		<input>Capacity</input>
		<input>UsedCapacity</input>
		<group-by>device</group-by>
		<group-by>serialnb</group-by>
		<group-by>sstype</group-by>
		<group-by>partid</group-by>
		<group-by>parttype</group-by>
		<filter>device &amp; serialnb &amp; parttype=='Storage Pool'</filter>
		<jexl-formula>
			<![CDATA[Capacity - UsedCapacity]]>
		</jexl-formula>
	</result>
	<result name="CurrentUtilization" unit="%" time-window="300s">
		<input>Capacity</input>
		<input>UsedCapacity</input>
		<group-by>device</group-by>
		<group-by>serialnb</group-by>
		<group-by>sstype</group-by>
		<group-by>partid</group-by>
		<group-by>parttype</group-by>
		<filter>device &amp; serialnb &amp; parttype=='Storage Pool'</filter>
		<jexl-formula>
			<![CDATA[UsedCapacity / Capacity * 100 ]]>
		</jexl-formula>
	</result>
	<result name="FreeCapacity" unit="GB" time-window="300s">
		<input>Capacity</input>
		<input>UsedCapacity</input>
		<group-by>device</group-by>
		<group-by>serialnb</group-by>
		<group-by>sstype</group-by>
		<group-by>partid</group-by>
		<group-by>parttype</group-by>
		<filter>device &amp; serialnb &amp; parttype=='Snapshot'</filter>
		<jexl-formula>
			<![CDATA[Capacity - UsedCapacity]]>
		</jexl-formula>
	</result>
	<result name="CurrentUtilization" unit="%" time-window="300s">
		<input>Capacity</input>
		<input>UsedCapacity</input>
		<group-by>device</group-by>
		<group-by>serialnb</group-by>
		<group-by>sstype</group-by>
		<group-by>partid</group-by>
		<group-by>parttype</group-by>
		<filter>device &amp; serialnb &amp; parttype=='Snapshot'</filter>
		<jexl-formula>
			<![CDATA[UsedCapacity / Capacity * 100 ]]>
		</jexl-formula>
	</result>
</config>